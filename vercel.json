{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "css/*", "use": "@vercel/static" },
    { "src": "js/*", "use": "@vercel/static" },
    { "src": "images/*", "use": "@vercel/static" },
    { "src": "public/img/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/landingvendas.html", "dest": "/landingvendas.html" },
    { "src": "/landingleadsia.html", "dest": "/landingleadsia.html" },
    { "src": "/landinge-commerce.html", "dest": "/landinge-commerce.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}